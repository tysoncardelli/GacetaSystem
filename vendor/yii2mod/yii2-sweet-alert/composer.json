{
    "name": "yii2mod/yii2-sweet-alert",
    "description": "Alert widget based on SweetAlert extension {@link http://tristanedwards.me/sweetalert)",
    "type": "yii2-extension",
    "keywords": ["yii2", "sweet alert", "yii2 alert", "alert"],
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Chepurnoy",
            "email": "igorzfort@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "bower-asset/sweetalert" : "*"
    },
    "autoload": {
        "psr-4": {
            "yii2mod\\alert\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    }
}
